{
    "include": [
        "app"
    ],
    "pythonVersion": "3.12",
    "exclude": [
        ".venv",
        "venv",
        "node_modules",
        "__pycache__",
        "**/__pycache__",
        "build",
        "dist"
    ],
    "typeCheckingMode": "strict",
    "useLibraryCodeForTypes": true,
    "reportMissingTypeStubs": false,
    "stubPath": "./typings"
}