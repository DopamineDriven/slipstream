{
  "name": "@t3-chat-clone/root",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.12.3+sha512.467df2c586056165580ad6dfb54ceaad94c5a30f80893ebdec5a44c5aa73c205ae4a5bb9d5ed6bb84ea7c249ece786642bbb49d06a307df218d03da41c317417",
  "scripts": {
    "build:credentials": "turbo build --filter=@t3-chat-clone/credentials",
    "build:redis-service": "turbo build --filter=@t3-chat-clone/redis-service",
    "build:targeted": "pnpm build:ui && pnpm build:redis-service && pnpm build:credentials && pnpm build:ws-server",
    "build:types": "turbo build --filter=@t3-chat-clone/types",
    "build:ui": "turbo build --filter=@t3-chat-clone/ui",
    "build:web": "turbo build --filter=@t3-chat-clone/web",
    "build:ws-server": "turbo build --filter=@t3-chat-clone/ws-server",
    "changeset": "changeset",
    "clean": "git clean -xdf node_modules",
    "dev": "turbo dev --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,cts,mts,js,jsx,mjs,cjs,json,yaml,yml,css,html,md,mdx}\" --ignore-unknown --cache",
    "lint": "turbo lint",
    "prepare": "cd .. && husky turborepo/.husky",
    "typecheck": "turbo typecheck",
    "clean:house": "cd tooling/eslint && git clean -xdf node_modules .turbo && cd ../prettier && git clean -xdf node_modules .turbo && cd ../typescript && git clean -xdf .turbo node_modules && cd ../jest-presets && git clean -xdf node_modules .turbo && cd ../../packages/ui && git clean -xdf dist .turbo node_modules && cd ../types && git clean -xdf dist .turbo node_modules && cd ../redis-service && git clean -xdf dist .turbo node_modules && cd ../credentials-service && git clean -xdf node_modules .turbo dist && cd ../../apps/web && git clean -xdf node_modules .next .turbo && cd ../ws-server && git clean -xdf node_modules dist .turbo && cd ../.. && git clean -xdf node_modules pnpm-lock.yaml && pnpm install && pnpm build:targeted",
    "generate:base64": "openssl rand -base64 64",
    "generate:hex": "openssl rand -hex 64",
    "generate:config": "pnpm --filter=@t3-chat-clone/ui generate:config",
    "npm:registry": "npm set registry https://registry.npmjs.org",
    "run:web": "turbo dev --filter=@t3-chat-clone/web",
    "run:ws-server": "turbo dev --filter=@t3-chat-clone/ws-server",
    "latest:pnpm": "corepack use pnpm@latest",
    "update:pnpm": "curl -fsSL https://get.pnpm.io/install.sh | sh -"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@d0paminedriven/turbogen": "^5.6.0",
    "@t3-chat-clone/eslint-config": "workspace:*",
    "@t3-chat-clone/prettier-config": "workspace:*",
    "@t3-chat-clone/tsconfig": "workspace:*",
    "@types/node": "^24.0.6",
    "dotenv": "^17.0.0",
    "eslint": "^9.30.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vercel": "^44.2.7"
  },
  "prettier": "@t3-chat-clone/prettier-config",
  "engines": {
    "node": ">=22",
    "npm": ">=9",
    "pnpm": ">=9"
  }
}
