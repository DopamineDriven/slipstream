{
  "name": "@slipstream/root",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "scripts": {
    "build:credentials": "turbo build --filter=@slipstream/credentials",
    "build:encryption": "turbo build --filter=@slipstream/encryption",
    "build:key-validator": "turbo build --filter=@slipstream/key-validator",
    "build:redis-service": "turbo build --filter=@slipstream/redis-service",
    "build:storage-s3": "turbo build --filter=@slipstream/storage-s3",
    "build:targeted": "pnpm build:ui && pnpm build:redis-service && pnpm build:credentials && pnpm build:encryption && pnpm build:key-validator && pnpm build:types && pnpm build:storage-s3 && pnpm build:ws-server",
    "build:types": "turbo build --filter=@slipstream/types",
    "build:ui": "turbo build --filter=@slipstream/ui",
    "build:web": "turbo build --filter=@slipstream/web",
    "build:ws-server": "turbo build --filter=@slipstream/ws-server",
    "built:it": "bash ./manage.sh build",
    "changeset": "changeset",
    "clean": "git clean -xdf node_modules",
    "dev": "turbo dev --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,cts,mts,js,jsx,mjs,cjs,json,yaml,yml,css,html,md,mdx}\" --ignore-unknown --cache",
    "lint": "turbo lint",
    "prepare": "cd .. && husky turborepo/.husky",
    "typecheck": "turbo typecheck",
    "clean:house": "bash ./manage.sh clean:house",
    "list:packages": "bash ./manage.sh list",
    "generate:base64": "openssl rand -base64 64",
    "generate:hex": "openssl rand -hex 64",
    "generate:config": "pnpm --filter=@slipstream/ui generate:config",
    "npm:registry": "npm set registry https://registry.npmjs.org",
    "run:web": "turbo dev --filter=@slipstream/web",
    "run:ws-server": "turbo dev --filter=@slipstream/ws-server",
    "latest:pnpm": "corepack use pnpm@latest",
    "wsl2:info": "lsb_release -a"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@d0paminedriven/turbogen": "^5.6.0",
    "@slipstream/eslint-config": "workspace:*",
    "@slipstream/prettier-config": "workspace:*",
    "@slipstream/tsconfig": "workspace:*",
    "@types/node": "^24.4.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "knip": "^5.63.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "vercel": "^48.0.0"
  },
  "prettier": "@slipstream/prettier-config",
  "engines": {
    "node": ">=22",
    "npm": ">=9",
    "pnpm": ">=9"
  }
}
