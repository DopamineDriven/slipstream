{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env.*local","packages/credentials-service/.env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  },
  "ui": "stream",
  "globalEnv": [
    "ANTHROPIC_API_KEY",
    "AUTH_GITHUB_ID",
    "AUTH_GITHUB_SECRET",
    "AUTH_GOOGLE_ID",
    "AUTH_GOOGLE_SECRET",
    "AUTH_SECRET",
    "AUTH_URL",
    "AWS_ACCESS_KEY",
    "AWS_ACCESS_KEY_SECRET",
    "AWS_CREDENTIAL_SECRET_ID",
    "AWS_REGION",
    "AWS_SECRET_ACCESS_KEY",
    "AWS_SECRETS_ID",
    "CLAUDE_CODE_OAUTH_TOKEN",
    "COREPACK_ENABLE_STRICT",
    "DATABASE_URL",
    "DATABASE_URL_UNPOOLED",
    "DIRECT_URL",
    "ENCRYPTION_KEY",
    "FASTAPI_URL",
    "GA_MEASUREMENT_PROTOCOL_SECRET",
    "GEMINI_API_KEY",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "GITHUB_PAT",
    "GOOGLE_API_KEY",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEXT_PUBLIC_GA_MEASUREMENT_ID",
    "NEXT_PUBLIC_GA_STREAM_ID",
    "JWT_SECRET",
    "MOTION_PLUS_PERSONAL_ACCESS_TOKEN",
    "MY_GITHUB_PAT",
    "NEXT_PUBLIC_WS_URL",
    "NPM_RC",
    "NODE_ENV",
    "OPEN_AI_KEY",
    "OPENAI_API_KEY",
    "PGDATABASE",
    "PGHOST",
    "PGHOST_UNPOOLED",
    "PGPASSWORD",
    "PGUSER",
    "PORT",
    "PORT_ONE",
    "PORT_THREE",
    "PORT_TWO",
    "POSTGRES_DATABASE",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL",
    "POSTGRES_URL_NO_SSL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD",
    "R2_ACCESS_KEY",
    "R2_ACCESS_KEY_ID",
    "R2_ACCOUNT_ID",
    "R2_ACCOUNT_TOKEN",
    "R2_BUCKET",
    "R2_ENDPOINT_URL",
    "R2_EU_URL",
    "R2_NON_EU_URL",
    "R2_PUBLIC_URL",
    "R2_SECRET_ACCESS_KEY",
    "R2_SECRET_KEY",
    "REDIS_URL",
    "S3_API",
    "STACK_SECRET_SERVER_KEY",
    "VERCEL_ENV",
    "WS_HOSTNAME",
    "X_AI_KEY"
  ]
}
