{
  "name": "@t3-chat-clone/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "prettier": "@t3-chat-clone/prettier-config",
  "scripts": {
    "dev": "next dev -p 3030 --turbo",
    "build": "next build",
    "format": "prettier --write \"**/*.{ts,tsx,cts,mts,js,jsx,mjs,cjs,json,yaml,yml,css,html,md,mdx,graphql,gql}\" --ignore-unknown --cache",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "pnpm db:generate",
    "db:generate": "prisma generate --no-engine",
    "db:migrate": "dotenv -- pnpm db:migrate:workup",
    "db:migrate:workup": "DATABASE_URL=$DATABASE_URL_UNPOOLED prisma migrate dev --skip-generate",
    "db:deploy": "dotenv -- pnpm db:deploy:workup",
    "db:deploy:workup": "DATABASE_URL=$DATABASE_URL_UNPOOLED prisma migrate deploy",
    "db:push": "dotenv -- pnpm db:push:workup",
    "db:push:workup": "DATABASE_URL=$DATABASE_URL_UNPOOLED prisma db push",
    "prisma:studio": "prisma studio --browser none --port 7777",
    "output:md": "tsx src/utils/to-md.ts --target root false"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@t3-chat-clone/ui": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "motion": "^12.19.1",
    "motion-plus": "^1.3.2",
    "nanoid": "^5.1.5",
    "next": "^15.3.4",
    "next-auth": "^5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-resizable-panels": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "vaul": "^1.1.2",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@d0paminedriven/fs": "^6.1.0",
    "@playwright/test": "^1.53.1",
    "@t3-chat-clone/eslint-config": "workspace:*",
    "@t3-chat-clone/prettier-config": "workspace:*",
    "@t3-chat-clone/tsconfig": "workspace:*",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/gtag.js": "^0.0.20",
    "@types/js-cookie": "^3.0.6",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/unist": "^3.0.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^2.2.2",
    "autoprefixer": "^10.4.21",
    "colorjs.io":"^0.5.2",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "dotenv-expand": "^12.0.2",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "mdast": "^3.0.0",
    "motion-dom": "^12.19.0",
    "motion-utils": "^12.19.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.1",
    "prisma": "^6.10.1",
    "rehype-katex": "^7.0.1",
    "rehype-pretty-code": "^0.14.1",
    "rehype-react": "^8.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.10",
    "tailwindcss-motion": "^1.1.1",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "urlpattern-polyfill": "^10.1.0",
    "webpack": "^5.99.9"
  }
}
