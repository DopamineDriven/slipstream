{
  "name": "@slipstream/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "prettier": "@slipstream/prettier-config",
  "scripts": {
    "dev": "next dev -p 3030 --turbo",
    "prebuild": "rm -rf dist",
    "build": "next build",
    "format": "prettier --write \"src/**/*.{ts,tsx,cts,mts,js,jsx,mjs,cjs,json,yaml,yml,css,html,md,mdx,graphql,gql}\" --ignore-unknown --cache",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "postinstall": "pnpm db:generate",
    "db:generate": "prisma generate --no-engine",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio --browser none --port 7777",
    "output:md": "tsx src/utils/to-md.ts --target root false"
  },
