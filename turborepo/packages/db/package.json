{
  "name": "@t3-chat-clone/db",
  "version": "1.0.0",
  "description": "prisma db",
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts",
          "dist/client.d.ts"
        ]
      }
    }
  },
  "license": "MIT",
  "source": "src/client.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts",
        "dist/*/index.d.ts",
        "dist/client.d.ts"
      ]
    }
  },
  "type": "module",
  "module": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "exports": {
    ".": "./dist/client.js"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "dev": "pnpm build",
    "lint": "eslint",
    "clean": "git clean -xdf .turbo dist node_modules",
    "fix": "npm pkg fix",
    "postinstall": "pnpm db:generate",
    "db:generate": "prisma generate --no-engine",
    "db:migrate": "dotenv -- pnpm db:migrate:workup",
    "db:migrate:workup": "DATABASE_URL=$DATABASE_URL_UNPOOLED prisma migrate dev --skip-generate",
    "db:deploy": "dotenv -- pnpm db:deploy:workup",
    "db:deploy:workup": "DATABASE_URL=$DATABASE_URL_UNPOOLED prisma migrate deploy",
    "db:push": "dotenv -- pnpm db:push:workup",
    "db:push:workup": "DATABASE_URL=$DATABASE_URL_UNPOOLED prisma db push",
    "prisma:studio": "prisma studio --browser none --port 7777"
  },
  "dependencies": {
    "@prisma/extension-accelerate": "^2.0.1",
    "@prisma/client": "^6.10.1",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "dotenv-expand": "^12.0.2"
  },
  "devDependencies": {
    "@d0paminedriven/fs": "^6.1.0",
    "@t3-chat-clone/eslint-config": "workspace:*",
    "@t3-chat-clone/prettier-config": "workspace:*",
    "@t3-chat-clone/tsconfig": "workspace:*",
    "@types/node": "^24.0.3",
    "chokidar": "^4.0.3",
    "eslint": "^9.29.0",
    "jiti": "^2.4.2",
    "prettier": "^3.6.0",
    "prisma": "^6.10.1",
    "terser": "^5.43.1",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "prettier": "@t3-chat-clone/prettier-config"
}
