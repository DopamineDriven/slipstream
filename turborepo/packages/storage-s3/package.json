{
  "name": "@slipstream/storage-s3",
  "version": "1.0.0",
  "description": "convenient s3 storage helpers",
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ],
        "s3": [
          "dist/s3/index.d.ts"
        ],
        "utils": [
          "dist/utils/index.d.ts"
        ],
        "types": [
          "dist/types/index.d.ts"
        ]
      }
    }
  },
  "license": "MIT",
  "source": "src/index.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ],
      "s3": [
        "dist/s3/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "types": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./utils": "./dist/utils/index.js",
    "./s3": "./dist/s3/index.js",
    "./types": "./dist/types/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build",
    "lint": "eslint",
    "clean": "git clean -xdf .turbo dist node_modules",
    "format": "prettier --write \"src/**/*.{ts,cts,mts,js,mjs,cjs,json,yaml,yml,css,html,md}\" --ignore-unknown --cache",
    "fix": "npm pkg fix"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-s3": "^3.892.0",
    "@aws-sdk/credential-providers": "^3.892.0",
    "@aws-sdk/lib-storage": "^3.892.0",
    "@aws-sdk/s3-request-presigner": "^3.892.0",
    "@aws-sdk/types": "^3.887.0",
    "@slipstream/types": "workspace:*",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@d0paminedriven/fs": "^7.6.0",
    "@slipstream/eslint-config": "workspace:*",
    "@slipstream/prettier-config": "workspace:*",
    "@slipstream/tsconfig": "workspace:*",
    "@types/node": "^24.5.2",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "terser": "^5.44.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "prettier": "@slipstream/prettier-config"
}
